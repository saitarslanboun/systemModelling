package HM.restaurant_game;

import java.io.PrintStream;

//import junit.framework.Assert;



import org.junit.Assert;
import org.junit.Test;
import org.mockito.Mockito;

//@SuppressWarnings("deprecation")
public class Step1Test {
	
	Restaurant restaurant;
	Player player;
	
	@Test
	public void testThe_is_created_with_the_name() {
		restaurant = new Restaurant();
		player = new Player();
		restaurant.setRestaurantName("UT Restaurant (UTR)");
		Assert.assertEquals("UT Restaurant (UTR)", restaurant.getRestaurantName());
		Assert.assertEquals("Naved", player.getPlayerName());
		
	}
	
}
